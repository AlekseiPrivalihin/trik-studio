<?xml version="1.0" encoding="utf-8"?>
<metamodel xmlns="http://schema.real.com/schema/">
	<include>../commonMetamodels/kernelMetamodel.xml</include>
    <include>../commonMetamodels/basicBehaviorsMetamodel.xml</include>
    <include>../commonMetamodels/basicActionsMetamodel.xml</include>
    <namespace>UML 2.0</namespace>
    <diagram displayedName="Редактор диаграмм баз данных" nodeName="DatabaseDiagram" name="DatabaseEditor">
        <nonGraphicTypes>
            <enum displayedName="Строковый тип" name="StringType">
                <value>CHAR</value>
                <value>VARCHAR</value>
                <value>CLOB</value>
            </enum>
            <enum displayedName="Числовой тип" name="NumType">
                <value>INTEGER</value>
                <value>SMALLINT</value>
                <value>BIGINT</value>
                <value>NUMERIC</value>
                <value>DECIMAL</value>
                <value>REAL</value>
                <value>DOUBLE PRECISION</value>
                <value>DOUBLE</value>
            </enum>
            <enum displayedName="Логический тип" name="BoolType">
                <value>TRUE</value>
                <value>FALSE</value>
                <value>UNKNOWN</value>
            </enum>
        </nonGraphicTypes>
        <graphicTypes>
            <node displayedName="Диаграмма баз данных" description="Корневой элемент создания диаграммы описания схемы базы данных. Для её создания перенесите этот элемент в обозреватель" name="DatabaseDiagram">
                <graphics>
                    <picture sizex="48" sizey="48">
                        <image y1="0" name="images/table.png" x1="0" y2="47" x2="47"/>
                    </picture>
                    <labels>
                        <label x="0" y="0" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                </logic>
            </node>
            <node displayedName="Таблица" description="Новая таблица" name="Table">
                <graphics>
                    <picture sizex="150" sizey="180">
                        <rectangle fill="#f0f8ff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="180" fill-style="solid" x2="150"/>
                    </picture>
                    <labels>
                        <label x="0" y="-25" textBinded="name"/>
                    </labels>
                   <ports>
                        <linePort>
                            <start startx="0" starty="30"/>
                            <end endx="0" endy="150"/>
                        </linePort>
                        <linePort>
                            <start startx="30" starty="0"/>
                            <end endx="120" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="150" starty="30"/>
                            <end endx="150" endy="150"/>
                        </linePort>
                        <linePort>
                            <start startx="30" starty="180"/>
                            <end endx="120" endy="180"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <properties>
                            <forestalling size="25"/>
                            <childrenForestalling size="5"/>
                            <sortContainer/>
                            <banChildrenMove/>
                            <minimizeToChildren/>
                            <maximizeChildren/>
                        </properties>
                    </container>
                </logic>
            </node>
            <node displayedName="Столбец строкового типа" name="ColumnString">
                <graphics>
                    <picture sizex="200" sizey="20">
                        <rectangle fill="#f0fff0" stroke-style="solid" stroke="#6495ed" y1="0" stroke-width="1" x1="0" y2="20" fill-style="solid" x2="200"/>
                    </picture>
                    <labels>
                        <label x="5" y="1" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Строковый тип" type="StringType" name="StringTypeProperty">
                            <default>CHAR</default>
                        </property>
						<property type="bool" name="is Primary Key">
							<default>false</default>
						</property>
						<property displayedName="Максимальное число символов" type="int" name="maxtypeLength"/>
                    </properties>
                </logic>
            </node>
            <node displayedName="Столбец числового типа" name="ColumnNumber">
                <graphics>
                    <picture sizex="200" sizey="20">
                        <rectangle fill="#f0fff0" stroke-style="solid" stroke="#f08080" y1="0" stroke-width="1" x1="0" y2="20" fill-style="solid" x2="200"/>
                    </picture>
                    <labels>
                        <label x="5" y="1" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Числовой тип" type="NumType" name="NumTypeProperty">
                            <default>INTEGER</default>
                        </property>
						<property type="bool" name="is Primary Key">
							<default>false</default>
						</property>
						<property displayedName="Число знаков" type="int" name="numberOfSigns"/>
						<property displayedName="Число знаков после запятой" type="int" name="numberOfSignAfterPoint"/>
                    </properties>
                </logic>
            </node>
            <node displayedName="Столбец логического типа" name="ColumnLogical">
                <graphics>
                    <picture sizex="200" sizey="20">
                        <rectangle fill="#f0fff0" stroke-style="solid" stroke="#006400" y1="0" stroke-width="1" x1="0" y2="20" fill-style="solid" x2="200"/>
                    </picture>
                    <labels>
                        <label x="5" y="1" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Значение по умолчанию" type="BoolType" name="DefaultValue">
                            <default>FALSE</default>
                        </property>
						<property type="bool" name="is Primary Key">
							<default>false</default>
						</property>
                    </properties>
                </logic>
            </node>
            <node displayedName="Столбец типа дата-время" name="ColumnDate">
                <graphics>
                    <picture sizex="200" sizey="20">
                        <rectangle fill="#f0fff0" stroke-style="solid" stroke="#483d8b" y1="0" stroke-width="1" x1="0" y2="20" fill-style="solid" x2="200"/>
                    </picture>
                    <labels>
                        <label x="5" y="1" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Тип" type="string" name="TypeProperty"/>
						<property type="bool" name="is Primary Key">
							<default>false</default>
						</property>
                    </properties>
                </logic>
            </node>
            <node displayedName="Столбец интервального типа" name="ColumnInterval">
                <graphics>
                    <picture sizex="200" sizey="20">
                        <rectangle fill="#f0fff0" stroke-style="solid" stroke="#808080" y1="0" stroke-width="1" x1="0" y2="20" fill-style="solid" x2="200"/>
                    </picture>
                    <labels>
                        <label x="5" y="1" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Тип" type="string" name="IntervalProperty"/>
						<property type="bool" name="is Primary Key">
							<default>false</default>
						</property>
                    </properties>
                </logic>
            </node>
            <edge displayedName="Связь" name="Connection">
                <graphics>
                    <lineType type="solidLine"/>
                   <labels>
                        <label textBinded="Foreign Key"/>
                    </labels>
                </graphics>
                <logic>
                    <associations endType="empty_arrow" beginType="no_arrow">
                        <association beginName="assotiationBegin" endName="assotiationEnd"/>
                    </associations>
                    <properties>
                        <property displayedName="Foreign Key Name" type="string" name="Foreign Key"/>
                    </properties>
                </logic>
            </edge>
        </graphicTypes>
    </diagram>
</metamodel>
